{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#A22929",
          "style": "plain",
          "template": "[{{ .Meaning }}]",
          "type": "exit"
        },
        {
          "foreground": "#FF9D00",
          "style": "plain",
          "template": " \uf0e7 ",
          "type": "root"
        },
        {
          "foreground": "#0088ff",
          "properties": {
            "style": "agnoster_full"
          },
          "style": "plain",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#ffc600",
          "properties": {
            "branch_gone_icon": "\u274e",
            "branch_identical_icon": "\uf14a"
          },
          "style": "plain",
          "template": "on {{ .HEAD }} ",
          "type": "git"
        },
        {
          "foreground": "#ff628c",
          "style": "plain",
          "template": "\u276f ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 2
}
